{
  "name": "react-reflow",
  "version": "0.1.3",
  "description": "State container for React apps",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:umd",
    "build:commonjs": "babel src/index.js --out-file lib/reflow.commonjs.js",
    "build:umd": "webpack src/index.js lib/reflow.js",
    "update-dependencies": "node config/dependencies.js"
  },
  "license": "MIT",
  "devDependencies": {
    "webpack": "^1.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-cli": "^6.16.0"
  },
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamssen/react-reflow.git"
  },
  "bugs": {
    "url": "https://github.com/iamssen/react-reflow/issues"
  },
  "homepage": "https://github.com/iamssen/react-reflow",
  "main": "lib/reflow.commonjs.js",
  "peerDependencies": {
    "react": "^15.3.2"
  }
}