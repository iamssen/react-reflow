{
  "name": "react-reflow-ghpages",
  "version": "0.1.0",
  "description": "Github pages",
  "scripts": {
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 4200 --host 0.0.0.0 --content-base src",
    "start": "npm run server",
    "update-dependencies": "node config/dependencies.js"
  },
  "license": "MIT",
  "devDependencies": {
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1",
    "copy-webpack-plugin": "^3.0.1",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "source-map-loader": "^0.1.5",
    "less-loader": "^2.2.3",
    "css-loader": "^0.25.0",
    "postcss-loader": "^0.13.0",
    "autoprefixer": "^6.5.0",
    "precss": "^1.4.0",
    "less": "^2.7.1",
    "style-loader": "^0.13.1",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "@types/react": "^0.14.37",
    "@types/react-dom": "^0.14.17",
    "@types/whatwg-fetch": "^0.0.31",
    "glob": "^7.1.0"
  },
  "dependencies": {
    "react-reflow": "../react-reflow",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "ie-shim": "^0.1.0",
    "core-js": "^2.4.1",
    "whatwg-fetch": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamssen/react-reflow.git"
  },
  "bugs": {
    "url": "https://github.com/iamssen/react-reflow/issues"
  },
  "homepage": "https://github.com/iamssen/react-reflow"
}